#include <iostream>
using namespace std;
 
int main()
{
    int x, y, subtract;
    
    cout << "Enter two integers for subtract: ";
    cin >> x >> y;
 
    // the subtracted value of two numbers stored in the third variable
    subtract = x - y;
 
    // Prints subtracted value 
    cout << x << " - " <<  y << " = " << subtract;     
 
    return 0;
}
